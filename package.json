{
  "name": "rodydavis",
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {
    "@lit-labs/preact-signals": "^1.0.2",
    "@preact/signals-core": "^1.8.0",
    "lit": "^3.2.1",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/bun": "^1.1.16",
    "ts-to-jsdoc": "^2.4.0"
  },
  "scripts": {
    "dev": "go run . serve",
    "ts:src:mixwith": "bun ts-to-jsdoc src/mixwith.ts -o pb_public/js -f",
    "ts:src:mixins": "bun ts-to-jsdoc src/mixins.ts -o pb_public/js -f",
    "build": "bun run build:signals && bun run build:lit && bun run build:ts",
    "build:signals": "bun build --minify ./deps/signals.ts --outfile ./pb_public/js/signals.min.js",
    "build:lit": "bun build --minify ./deps/lit.ts --outfile ./pb_public/js/lit.min.js",
    "build:ts": "npx tsc src/**/*.ts --outDir pb_public/js --target esnext --noCheck --skipLibCheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodydavis/rodydavis.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rodydavis/rodydavis/issues"
  },
  "homepage": "https://github.com/rodydavis/rodydavis#readme",
  "description": ""
}
